{
  "name": "@sl-apps/public-app",
  "description": "The new sortlist public-app application",
  "homepage": "https://github.com/sortlist/javascript-labs/apps/public-app",
  "author": {
    "name": "Vanvelthem SÃ©bastien",
    "url": "https://github.com/belgattitude"
  },
  "version": "0.4.6",
  "private": true,
  "scripts": {
    "dev": "run-s crowdin:locales:create:empty dev:next-js",
    "dev:next-js": "next dev",
    "build": "next build",
    "build:production": "PUBLISH=true node ./scripts/download-crowdin-locales.js && yarn build",
    "export": "next export",
    "clean": "rimraf --no-glob ./.next ./out ./coverage",
    "start": "next start",
    "typecheck": "tsc -p . --noEmit",
    "test": "yarn test:unit && yarn test:integ",
    "test:watch": "jest --config jest.config.js --watch",
    "test:unit": "jest --config jest.config.js --projects tests/unit",
    "test:unit:coverage": "jest --config jest.config.js --projects tests/unit --coverage",
    "test:integ": "jest --config jest.config.js --projects tests/integ",
    "test:integ:coverage": "jest --config jest.config.js --projects tests/integ --coverage",
    "lint": "yarn lint:code && yarn lint:scss",
    "lint:code": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:scss": "stylelint 'src/**/*.scss' --syntax scss --allow-empty-input",
    "fix:staged": "lint-staged --allow-empty",
    "fix:all": "yarn fix:all:code && yarn fix:all:scss",
    "fix:all:code": "eslint . --ext .ts,.tsx,.js,.jsx --fix --rule 'react-hooks/exhaustive-deps: off' --fix",
    "fix:all:scss": "stylelint 'src/**/*.scss' --syntax scss --allow-empty-input --fix",
    "prettier": "prettier --write \"./src/**/*.{ts,tsx,scss,js,json,less}\"",
    "prettier:all": "prettier --write \"./src/**/*.{ts,tsx,scss,js,json,less}\"",
    "check:config": "run-p check:config:*",
    "check:config:template-env": "node ./scripts/check-template-env.js",
    "check:es": "run-s clean build export check:es:*",
    "check:es:build": "es-check es5 './.next/static/**/*.js' -v",
    "check:es:export": "es-check es5 './out/**/*.js' -v",
    "analyze": "cross-env ANALYZE=true next build",
    "coverage:publish": "codecov --root=../../ -F publicApp",
    "crowdin:locales:create:empty": "node ./scripts/create-empty-locales.js",
    "crowdin:locales:create:populated": "node ./scripts/download-crowdin-locales.js",
    "crowdin:locales:purge": "sh ./scripts/purge-crowdin-locales.sh",
    "deps:check": "npx npm-check-updates --dep prod,dev,optional",
    "deps:update": "npx npm-check-updates --dep prod,dev,optional -u",
    "storybook": "start-storybook --docs -p 9009 -s public"
  },
  "browserslist": {
    "production": [
      ">1%",
      "ie >= 11",
      "edge >= 14",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@desync/core": "0.4.6"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@material-ui/core": "^4.9.8",
    "@material-ui/icons": "^4.9.1",
    "@mdx-js/loader": "^1.5.8",
    "@mdx-js/react": "^1.5.8",
    "@next/mdx": "^9.3.4",
    "@reduxjs/toolkit": "^1.3.2",
    "@sentry/browser": "5.15.4",
    "@sentry/integrations": "5.15.4",
    "@sentry/node": "5.15.4",
    "@sindresorhus/is": "^2.1.0",
    "@sortlist/error-flow": "^0.4.5",
    "@svgr/webpack": "^5.3.0",
    "@zeit/next-sass": "^1.0.1",
    "abort-controller": "^3.0.0",
    "abortcontroller-polyfill": "^1.4.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "clsx": "^1.1.0",
    "cors": "^2.8.5",
    "dequal": "^1.0.0",
    "dotenv": "^8.2.0",
    "formik": "^2.1.4",
    "i18next": "^19.3.4",
    "ky": "^0.19.0",
    "ky-universal": "^0.5.0",
    "loglevel": "^1.6.7",
    "next": "9.3.4",
    "next-absolute-url": "^1.2.2",
    "next-fonts": "^1.0.3",
    "next-images": "^1.4.0",
    "next-seo": "^4.4.0",
    "next-transpile-modules": "^3.1.0",
    "react": "16.13.1",
    "react-clamp-lines": "^3.0.2",
    "react-dom": "16.13.1",
    "react-i18next": "^11.3.4",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "shelljs": "0.8.3",
    "yup": "^0.28.3"
  },
  "engines": {
    "node": ">=10.13",
    "yarn": ">=1.19.0 <2.0.0",
    "npm": "please-use-yarn"
  }
}
